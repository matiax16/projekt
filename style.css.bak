header, nav, main, footer
{
display: block;
width: 1078px;
}
body {
 color: black;
 background-color:#000000;
 margin: 0px;
 text-align: center;
}
header {
 background-color:#4682B4;
 height: 100px;
 position:relative;
}
nav {
 background-color:#AFEEEE;
 height: 150px;
} 
main {
 background-color:#FFFAF0;
 height: auto;
 overflow:hidden; 
} 
footer {
 background-color:#6495ED;
 height: 100px
 clear: both; 
}
#col-center {
background-color:#003acc;
width: 500;
height: 150px;
}
#col-left {
background-color:#006acc;
width: 200px;
 height: 200px;
float: left;
}
#container {
 position: relative;
margin: auto;
 width: 1078px;
 height: auto;
 overflow: hidden;
border: 1px solid #ffffff;
} 
#logo img {
position: absolute;
width: 200px;
height: 200px;
border: 2px dotted #006acc;
}

#logo img
#styl td{
	border-collapse: collapse;
    cellspacing: none;
	border-color: black;
	border-spacing: none;
	border-width: 1px;
	padding-left:20px;
	padding-right:20px;
	text-align: left;
	}

#styl tr:nth-child(even){background-color: yellow;}
#styl tr:nth-child(odd) {background-color: blue;}

#styl tr:hover {background-color: white;}
#styl th 
{
	background-color: white-space;
}